[infer (load_plugin "plugins/venstan.py")]
[assume stan_prog "
data {
  int N;
  real y[N];
}
parameters {
  real mu;
  real sigma;
}
model {
  y ~ normal(mu, sigma);
}
generated quantities {
  real y_out[N];
  y_out ~ normal(mu, sigma);
}"]
[assume inputs '(("N" "Int"))]
[assume outputs '(("y" "UArray Number" "y_out"))]
[assume stan_sp (make_ven_stan stan_prog inputs outputs)]
[predict (stan_sp 5)]
