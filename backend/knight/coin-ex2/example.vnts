flip_coins = (n) ~> {
  weight = beta(2, 2);
  replicate2(n, () ~> { flip(weight) })
};

constrain_coin_flipper_trace = (n) -> {
  t1 = T{};
  for_each((k) -> { t1[1, "app", k, 0, "app"] := true },
    range(5));
  t1
};

extract_weight = (mechanism) -> {
  @mechanism[0, "def", "app"]
};

